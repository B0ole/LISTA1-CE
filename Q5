#include <stdio.h>


int main()
{
	int op;
	double a,b;
	printf("");
	scanf("%lf %lf ", &a, &b);
	printf("");
	scanf("%d", &op);
	
	switch(op ){
	    case 1:
	    printf("%.3lf", a+b);
	    break;
	  
	    case 2:
	    printf("%.3lf", a-b);
	    break;
	    case 3:
	    if((a==0)||(b==0)){
	        printf("erro");}
	    else{printf("%.3lf", a*b);}
	    
	   break;
	   
	   case 4:
	   if(/*(a==0)*||*/(b==0)){
	    printf("erro");}
	    else{printf("%.3lf", a/b);
	    break;
	    }
	    
	    
	    
	    
	    
	    
	    
	    }
	
	
	
	
	
	
	return 0;
}
