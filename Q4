#include <stdio.h>


int main(){

char TIPO;
double L;


//printf("Qual TIPO?:");
scanf("%s", &TIPO);
//printf("qual o L, raio da TIPO:");
scanf("%lf", &L);




if(L>0){
    if((TIPO=='T')||(TIPO=='t')||(TIPO=='q')||(TIPO=='Q')||(TIPO=='c')||(TIPO=='C')){

switch(TIPO){
case ('T'):
    printf("%.2lf\nTriangulo equilatero", (1.7/4)*L*L);
    break;

case ('t'):
    printf("%.2lf\nTriangulo equilatero", (1.7/4)*L*L);
    break;

case('c'):
    printf("%.2lf\nCirculo",L*3.14*L);
    break;

case('C'):
    printf("%.2lf\nCirculo ",L*3.14*L);
    break;
case('Q'):
    printf("%.2lf\nQuadrado", L*L);
    break;
case('q'):
    printf("%.2lf\nQuadrado", L*L);    
}
}
else{printf("Entrada invalida!");}
}
else{printf("Entrada invalida!");}

return 0;
}











    

