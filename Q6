#include <stdio.h>

int main()
{
    int horas, minutos, minutos_adicionais;

    // lê a entrada no formato "horas:minutos" e os minutoss adicionais
    printf("");
    scanf("%d:%d", &horas, &minutos);
    printf("");
    scanf("%d", &minutos_adicionais);

    // adiciona os minutoss adicionais à horas e minutos
    minutos += minutos_adicionais;
    horas += minutos / 60;
    minutos %= 60;
    horas %= 24;

    // imprime a horas e minutos atualizados
    printf("%02d:%02d\n", horas, minutos);

    return 0;
}
