#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void sortList(char characters[3]) {
    int i, j;
    char temp;
    for (i = 0; i < 2; i++) {
        for (j = i + 1; j < 3; j++) {
            if (characters[i] > characters[j]) {
                temp = characters[i];
                characters[i] = characters[j];
                characters[j] = temp;
            }
        }
    }
}

int main() {
    char first_list[3], second_list[3];
    int i;

    printf("Enter 3 letters for first list: ");
    for (i = 0; i < 3; i++) {
        scanf(" %c", &first_list[i]);
    }

    printf("Enter 3 letters for second list: ");
    for (i = 0; i < 3; i++) {
        scanf(" %c", &second_list[i]);
    }

    sortList(first_list);
    sortList(second_list);

    printf("First list in alphabetical order: ");
    for (i = 0; i < 3; i++) {
        printf("%c ", first_list[i]);
    }
    printf("\n");
    printf("Second list in alphabetical order: ");
    for (i = 0; i < 3; i++) {
        printf("%c ", second_list[i]);
    }

    return 0;
}
