#include <stdio.h>



int main(){
    
    double z1,z2;
    printf("");
    scanf("%lf %lf", &z1, &z2);
    
    printf ("Produto: %.4lf \n", z1*z2);
    
    if(z2==0){
        printf("erro");
        }
    else{
        printf("Razao: %.4lf", z1/z2);}
    
    
    
    
    
    
    
   return 0; 
    
    }
